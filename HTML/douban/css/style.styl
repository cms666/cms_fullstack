

body, div, dl, dt, dd, ul, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, button, textarea, blockquote 
  margin 0
  padding 0
ul,ol
  list-style none
a:visited
  color #669
  text-decoration none

body
  margin-top 30px
body, td, th 
  font 12px Helvetica,Arial,sans-serif
  line-height 1.62
form
  padding 0
  margin 0
  border 0

input
  margin-right 3px
  vertical-align middle

.wrapper
  width 950px
  margin 0 auto
