#anony-nav 
    width 1080px
    margin 0 auto 25px
    .anony-nav-links
      float right 
      margin-top 6px
      li
        display inline-block
        zoom 1
        margin 0 14px 0 0px
        a
          display inline-block
          zoom 1
          width 40px
          height 20px
          overflow hidden
          margin 0
          line-height 20em
          outline none 
          vertical-align middle
          background transparent url(https://img3.doubanio.com/f/sns/8536084688879cb1137e8efdff30e59614ff2f86/pics/sns/anony_home/anony_nav_logo.png) no-repeat 100% 100%
        .lnk-book
          background-position 0 0
        .lnk-movie
          background-position -60px 0
        .lnk-music
          background-position -120px 0
        .lnk-evnets
          background-position -299px 0
        .lnk-group
          background-position -180px 0
        .lnk-read
          background transparent url(https://img3.doubanio.com/f/sns/f3ba00ef46f185989b70ac6841d9f4d67b6e3a25/pics/sns/anony_home/ic_read_home_nav.png) no-repeat 100% 100%
          background-size cover
        .lnk-fm
          background-position -241px 0
        .lnk-shijian
          margin-bottom 1px
          background transparent url(https://img3.doubanio.com/f/sns/b474dd402e4293943146a4789de3dae88ec96636/pics/sns/anony_home/ic_shijian_home_nav.png) no-repeat 100% 100%
          background-size cover
        .lnk-market
          width 42px
          background-position -421px 0
    h1
      display inline-block
      zoom 1
      vertical-align middle
      width 160px
      height 34px
      margin 0 20px 0 0
      padding 0
      font-size 12px
      font-weight normal
      background url(https://img3.doubanio.com/f/sns/19886d443852bee48de2ed91f4a3bdfdaf8c809c/pics/nav/logo_db.png) no-repeat 0 0
      overflow hidden
      a
        display block
        height 34px
        line-height 10em        
    .anony-srh
      position relative
      display inline-block
      zoom 1
      vertical-align middle
      width 270px
      height 30px
      border 1px solid #c3c3c3
      .inp
        display block
        height 30px
        overflow hidden
        input
          width 240px
          border none 
          font-size 13px
          height 20px
          padding 5px
          color #c3c3c3
      .bn
        position absolute
        right 0
        top 0
        width 30px
        height 30px
        overflow hidden
        cursor pointer
        background url(https://img3.doubanio.com/f/sns/f71f15922ebd7c0ff0ea0e7a25577529efd8981a/pics/icon/bn_srh_1.png) 50% 50% no-repeat
        input
          border none 
          background transparent
          overflow hidden
          width 30px
          height 30px
          font-size 0
          color transparent
          cursor pointer
#anony-reg-new
  margin-bottom 25px
  background-size contain
  background-position -webkit-calc(50% - 305px) top
  background-position calc(50% - 305px) top
  background-repeat no-repeat
  background-color #edf4ed
  .wrapper
    position relative
    padding 0 0 4px
    overflow visible
    zoom 1
    &:before
      content ""
      position absolute
      top 0
      bottom 0
      left 0
      right 0
      z-index 0
      background-repeat no-repeat
      background-position 0 -150px
    &:after
      content '\0020'
      display block
      clear both
    .login
      position relative
  .login
    width 300px
    float right 
    font-size 0
    letter-spacing -.31em
  .app
    position absolute
    height 120px
    top 0
    bottom 0
    margin auto 0 auto 475px
  .app-title
    margin 7px 0
    font-size 25px
    color #111111
    line-height 24px
    -webkit-font-smoothing antialiased 
    text-rendering optimizeLegibility
    span 
      color #111111
      margin-left 10px
      font-weight 100
  .app-slogin
    margin 0 0 18px
    font-size 0
    color #111111
    &:after
      content ''
      display inline-block
      width 100%
  .lnk-app
    display inline-block
    zoom 1
    padding 9px 21px
    margin-right 2px
    vertical-align top
    border-radius 2px
    font-size 12px
    line-height 1
  .app-qr
    display inline-block
    zoom 1  
    position relative
  .lnk-qr
    display inline-block
    zoom 1
    border 1px solid #48aaod
    border-radius 2px
  .app-qr-expand
    display none
    position absolute
    top 0
    left 0
    padding 20px 20px 5px
    border-radius 2px
    text-align center
    font-size 13px
    color #666
    background #fff
    line-height 13px
    white-space nowrap
    text-decoration none

